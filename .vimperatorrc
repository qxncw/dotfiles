cnoremap <C-p> <UP>
cnoremap <C-n> <DOWN>
nnoremap J <C-n>
nnoremap K <C-p>

hi Bell border: 0 none; background: #101010;
hi CmdLine background: #101010; color: #b9b9b9;
hi CmdLine>* font-family: "GohuFont", "Fixed";
hi CompDesc color: #b9b9b9; width: 50%;
hi CompIcon width: 12px; min-width: 12px; display: none; margin-right: .5ex;
hi CompItem background: inherit; color: #ccc;
hi CompItem[selected] background: #303030; color: #eee;
hi CompMsg font-style: italic; margin-left: 12px;
hi CompResult width: 45%; overflow: hidden;
hi CompTitle color: #b9b9b9; background: #101010; font-weight: bold;
hi CompTitle>* padding: 0 .5ex;
hi ContentSeparator border-top: none; color: #c3c3c3;
hi ErrorMsg color: #b9b9b9; background: #101010;
hi Filter color: #afafaf;
hi FrameIndicator background-color: #303030; opacity: 0.5; z-index: 999; position: fixed; top: 0; bottom: 0; left: 0; right: 0;
hi Function color: #b9b9b9; background: #101010;
hi Hint font-family: "GohuFont"; font-size: 14px; font-weight: bold; text-transform: uppercase; color: white; background-color: red; border-color: ButtonShadow; border-width: 0px; border-style: solid; padding: 0px 1px 0px 1px;
hi InfoMsg color: #b9b9b9; background: #101010;
hi LineNr color: #b9b9b9; background: #101010;
hi ModeMsg color: #b9b9b9; background: #101010;
hi MoreMsg color: #b9b9b9; background: #101010;
hi NonText color: #464646; padding-left: 2px;
hi Normal color: #b9b9b9; background: #101010;
hi Number color: #b9b9b9; background: #101010;
hi PromptText color: #b9b9b9; background: #101010; font-family: "GohuFont", "Fixed";
hi Question color: #b9b9b9; background: #101010;
hi StatusLine color: #b9b9b9; background: #101010;
hi String color: #b9b9b9; background: #101010;
hi Tag color: #b9b9b9; background: #101010;
hi Title color: #b9b9b9; background: #101010;
hi URL text-decoration: none; color: #ababab; background: inherit;
hi WarningMsg color: #b9b9b9; background: #101010;
hi Indicator color: #b9b9b9;

set nosb

set complete=b

style chrome://* <<EOM
	.liberator-container,
	#liberator-statusline > *,
	#liberator-completions-content,
	#liberator-multiline-output-content {
		font-size: 14px !important;
		font-family: "GohuFont", "Fixed" !important;
	}
	#liberator-status-ssl {
		display: none !important;
	}
EOM
